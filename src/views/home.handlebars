<div class="container">
  <div class="row">

    {{#each result.payload}}
      <div class="col-md-3">
        <div class="card text-center">
          <img
            src="{{this.thumbnails}}"
            class="img-fluid"
            alt="{{this.title}}"
          />
          <div class="card-body">
            <h5 class="card-title">{{this.title}}</h5>
            <p class="card-text">${{this.price}}</p>
          </div>
        </div>
      </div>
    {{/each}}

  </div>
</div>